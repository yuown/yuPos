<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns='http://www.ehcache.org/v3'
         xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core-3.0.xsd">

    <cache-template name="simple">
        <expiry>
            <ttl unit="seconds">3600</ttl>
        </expiry>
        <heap>100</heap>
    </cache-template>

    <cache alias="yuown.pos.domain.User" uses-template="simple"/>

    <cache alias="yuown.pos.domain.Authority" uses-template="simple"/>

    <cache alias="yuown.pos.domain.User.authorities" uses-template="simple"/>

    <cache alias="yuown.pos.domain.PersistentToken" uses-template="simple"/>

    <cache alias="yuown.pos.domain.User.persistentTokens" uses-template="simple"/>

    <cache alias="yuown.pos.domain.MultiList" uses-template="simple"/>

    <cache alias="yuown.pos.domain.MultiList.lists" uses-template="simple"/>

    <cache alias="yuown.pos.domain.Level" uses-template="simple"/>

    <cache alias="yuown.pos.domain.Level.levelLists" uses-template="simple"/>

    <cache alias="yuown.pos.domain.Level.elementLevels" uses-template="simple"/>

    <cache alias="yuown.pos.domain.Element" uses-template="simple"/>

    <cache alias="yuown.pos.domain.Element.elements" uses-template="simple"/>

    <cache alias="yuown.pos.domain.Element.parents" uses-template="simple"/>

    <cache alias="yuown.pos.domain.ListLevel" uses-template="simple"/>

    <cache alias="yuown.pos.domain.LevelElement" uses-template="simple"/>

    <cache alias="yuown.pos.domain.Configuration" uses-template="simple"/>

    <cache alias="yuown.pos.domain.Element.skuses" uses-template="simple"/>

    <cache alias="yuown.pos.domain.Sku" uses-template="simple"/>

    <!-- jhipster-needle-ehcache-add-entry -->
</config>
